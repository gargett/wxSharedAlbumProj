#touxiang image {
  width: 100%;
  height: 100%;
}
#timeLine {

}
#timeLine_header {
  padding: 0 30rpx 0;
  font-size:33.75rpx;
}

#timeLine_header span:nth-of-type(1) {
  color: #666666;
}
/* #timeLine_header span:nth-of-type(1)::after {
  content: '|';
  margin-left:15rpx;
  display: inline;
  color: #666666;
} */

#timeLine_header span:nth-of-type(2) {
  margin-left:15rpx;
}
#timeLinePic {
  background: transparent;
}
.timeLinePic_item {
  background: white;
  margin-bottom:21.75rpx;
  padding:20.25rpx20.25rpx;
}

.timeLinePic_item_user {
  padding:15rpx 0;
  position: relative;
}

.user_touxiag,
.userDateLocal,
.more {
  display: inline-block;
  vertical-align: middle;
}

.user_touxiag {
  width: 78rpx;
  height: 78rpx;
  border-radius:39rpx;
  border: 1rpx solid #d9d9d9;
  background: yellow;
}

.userDateLocal {
  height: 78rpx;
  margin-left:20.25rpx;
  overflow: hidden;
}
.userDateLocal0.userName {
  font-size:33.75rpx;
  font-weight: bold;
}
.userDateLocal0.dateAndLocal {
  font-size:26.25rpx;
  color: #999999;
}
.more {
  width: 78rpx;
  height: 78rpx;
  line-height: 78rpx;
  font-size: 52.5rpx;
  color: #20d575;
  text-align: center;
  position: absolute;
  top: -22.5rpx;
  bottom: 0;
  right: -37.5rpx;
  margin: auto;
  transform: rotate(90deg);
}

.timeLinePic_item_content {
  padding:15rpx 0;
  font-size:33.75rpx;
}

.timeLinePic_item_pics {
  width: 100%;
}

.timeLinePic_item_control {
  font-size: 0;
}
.zhuanfa, .comment, .like {
  width: 49%;
  height: 80.25rpx;
  text-align: center;
  line-height: 80.25rpx;
  display: inline-block;
  font-size:33.75rpx;
  color: #666666;
}
.zhuanfa img,
.comment img,
.like img {
  width:27.75rpx;
  height:27.75rpx;
  margin-right:9.75rpx;
  vertical-align: middle;
  margin-top: -.525rpx;
}
#see_more,
#add_my_album {
  width: 329.25rpx;
  height: 89.25rpx;
  position: fixed;
  border-radius: 167.25rpx;
  text-align: center;
  line-height: 89.25rpx;
  bottom:32.25rpx;
  font-size:36rpx;
}
#see_more img,
#add_my_album img {
  margin-right:7.5rpx;
  width:.4275rpx;
  height:.4275rpx;
  vertical-align: middle;
  margin-bottom:5.250000000000001rpx;
}
#see_more {
  left:30rpx;
  background: white;
  color:#20d575;
  border: 1rpx solid #20d575;
}

#add_my_album {
  right:30rpx;
  color: white;
  background: #20d575;
}
.timeLinePic_item_comment {
  font-size:27.75rpx;
  color: #666666;

}
.timeLinePic_item_comment text {
  color: black;
  margin-right:5.250000000000001rpx;
}
.timeLinePic_item_pics_item {
  width: 689.25rpx;
  margin: auto;
  overflow: hidden;
  margin-bottom:8.25rpx;
  font-size: 0;
  border-radius:20.25rpx;
}
.timeLinePic_item_pics_item image {
  width: 100%;
}
#banner {
  padding-top: 108.75rpx;
  position: relative;
}
#banner_bg {
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  z-index: 1;
}
#touxiang {
  width: 168.00000000000003rpx;
  height: 168.00000000000003rpx;
  border-radius: 84.00000000000001rpx;
  background-size: cover;
  margin: auto;
  overflow: hidden;
}
#info {
  font-size:24rpx;
  text-align: center;
  color: #333333;
  margin-bottom: 78rpx;
}
#touxiang, #info, #wenzi {
  z-index: 2;
  position: relative;
}
#user_name, #date_and_local {
  margin-top:21.75rpx;
}
#date::after {
  margin: 11.25rpx;
}
#wenzi {
  font-size:27.75rpx;
  font-weight: bold;
  text-align: left;
  padding-left:27.75rpx;
  white-space: pre;
}